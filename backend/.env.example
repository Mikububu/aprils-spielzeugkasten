# April's Spielzeugkasten Backend Environment Variables
# Copy this file to .env and fill in the values

PORT=3001
NODE_ENV=development

# Replicate API (FLUX Image Generation - NO CENSORSHIP, FREE TIER AVAILABLE)
# Get your API key from https://replicate.com/account/api-tokens
REPLICATE_API_KEY=your_replicate_api_key_here
REPLICATE_MODEL=flux-schnell  # or flux-dev for higher quality

# Supabase Configuration
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# CORS Origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000
